{
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "lib": ["dom"],
    "removeComments": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": [ "src/ui/*" ],
      "@pywalfox/*": [ "src/*" ],
      "@utils/*": [ "src/utils/*" ],
      "@config/*": [ "src/config/*" ],
      "@definitions": [ "src/definitions" ],
      "@communication/*": [ "src/communication/*" ]
    }
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}
